{
    "name": "alpinejs-devtools",
    "version": "0.0.4",
    "private": true,
    "description": "Simple Devtools for Alpine.js To Make Your Life Simpler",
    "homepage": "https://github.com/Te7a-Houdini/alpinejs-devtools",
    "scripts": {
        "dev": "rollup -c",
        "prod": "NODE_ENV=production rollup -c",
        "watch": "rollup -c -w",
        "test": "jest",
        "prepackage": "npm run prod",
        "package": "cd ./dist/chrome && zip -r ../alpine-devtools-$npm_package_version.zip . && rm -rf ../chrome"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/preset-env": "^7.9.5",
        "@fullhuman/postcss-purgecss": "^2.1.2",
        "@rollup/plugin-node-resolve": "^7.1.3",
        "@tailwindcss/ui": "^0.1.4",
        "@testing-library/dom": "^7.2.1",
        "@testing-library/jest-dom": "^5.5.0",
        "alpinejs": "^2.2.5",
        "jest": "^25.4.0",
        "rollup": "^2.6.1",
        "rollup-plugin-copy": "^3.3.0",
        "rollup-plugin-filesize": "^7.0.0",
        "rollup-plugin-postcss": "^2.5.0",
        "rollup-plugin-terser": "^5.3.0",
        "tailwindcss": "^1.9.6"
    },
    "dependencies": {}
}
